    "file": "LICENSE"
  },
  "authors": {
    "davidHigginbotham": "dhigginbotham@clearent.com"
  },
  "source": {
    "git": "https://github.com/clearent/ClearentIdtechIOSFrameworkPod.git",
    "tag": "0.1.0"
  },
  "platforms": {
    "ios": "8.0"
  },
  "public_header_files": [
    "ClearentIdtechIOSFrameworkPod/Assets/IDTech.framework/Versions/A/Headers/IDTech.h",
    "ClearentIdtechIOSFrameworkPod/Assets/ClearentIdtechIOSFramework/Clearent_VP3300.h",
    "ClearentIdtechIOSFrameworkPod/Assets/ClearentIdtechIOSFramework/ClearentDelegate.h",
